  "geo_domain_tests": {
    "description": "Geo-routing tests for CDN domains", 
    "tests": [
      {
        "domain": "cdn-geo.runonflux.io",
        "qtype": "A",
        "expected_contains": ["cdn-6.runonflux.io", "cdn-1.runonflux.io", "cdn-12.runonflux.io"],
        "description": "Should resolve to one of the CDN servers based on geo-location"
      }
    ]
  },
  "soa_tests": {
    "description": "SOA record tests",
    "tests": [
      {
        "domain": "app.runonflux.io",
        "qtype": "SOA", 
        "expected_contains": "ns1.runonflux.io"
      },
      {
        "domain": "app2.runonflux.io",
        "qtype": "SOA",
        "expected_contains": "ns1.runonflux.io" 
      },
      {
        "domain": "cdn-geo.runonflux.io",
